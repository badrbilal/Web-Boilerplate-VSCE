{
  "name": "web-boilerplate",
  "displayName": "Web Boilerplate",
  "description": "Creates boilerplate files (html, css, and js) for web app",
  "version": "1.0.0",
  "publisher": "jamesqquick",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createWebBoilerplate"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.createWebBoilerplate",
        "title": "Create Web Boilerplate"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.11.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
